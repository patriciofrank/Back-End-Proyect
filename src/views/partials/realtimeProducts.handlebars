<div>
 {{#if productsList}}
 <table id="table">
 <tr>
 <th>Nombre</th>
 <th>Precio</th>
 <th>Imagen</th>
 </tr>
 {{#each productsList}}
 <tr>
 <td>{{this.title}}</td>
 <td>{{this.price}}</td>
 <td>
 <img src={{this.thumbnail}} style="width: 150px;"></img>
 </td>
 </tr>
 {{/each}}
 </table>
 {{else}}
 <span>No hay productos</span>
 {{/if}}
 </div>
 {{>form}}

   


